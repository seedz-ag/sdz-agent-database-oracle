SELECT
        (CASE F2_FILIAL
            WHEN '010101' THEN '010101'
            WHEN '010102' THEN '010102'
            ELSE NULL
         END) "CNPJORIGEMDADOS",
       E1_FILORIG ,
       (E1_NUM || E1_PARCELA) TITULO ,
       (E1_CLIENTE || E1_LOJA) IDCLIENTE ,
       A1_NOME ,
       E1_EMISSAO ,
       E1_NUM ,
       E1_SERIE ,
       E1_VENCORI ,
       E1_VENCREA ,
       CAST(E1_VALLIQ  AS VARCHAR(10)) AS VALLIQ,
       CAST(E1_VALOR as VARCHAR(10)) AS VALOR,
       CAST(E1_JUROS AS VARCHAR(10)) AS JUROS,
       E1_DESCONT,
       E1_PARCELA,
        (CASE E1_STATUS
            WHEN 'A' THEN 'Aberto'
            WHEN 'B' THEN 'Liquidado'
            WHEN 'R' THEN 'Outros'
            ELSE NULL
         END) E1_STATUS ,
       E1_BAIXA ,
       E1_VEND1 ,
       E1_VEND2,
       E1_VEND3,
       E1_VEND4,
       (E1_FILIAL || E1_FILORIG || E1_NUM || E1_SERIE || E1_PARCELA || E1_CLIENTE || E1_LOJA || E1_EMISSAO) ID ,
       B.R_E_C_N_O_ ,
       B.D_E_L_E_T_ DELETADO ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_INCLUSAO ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_ALTERACAO
FROM SE1010 B
INNER JOIN SA1010 X ON X.A1_COD = B.E1_CLIENTE
AND X.A1_LOJA = B.E1_LOJA
AND X.D_E_L_E_T_ <> '*'
INNER JOIN SF2010 I ON I.F2_FILIAL = B.E1_FILORIG
AND I.F2_CLIENTE = B.E1_CLIENTE
AND I.F2_LOJA = B.E1_LOJA
AND I.F2_EMISSAO = B.E1_EMISSAO
AND I.D_E_L_E_T_ <> '*'
WHERE B.E1_USERLGI <> ' '
  AND (TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') >= '20211004'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') <= '20211011'
       OR TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') >= '20211004'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') <= '20211011')
ORDER BY B.R_E_C_N_O_ ASC
 