SELECT E1_FILIAL ,
       A1_FILIAL ,
       F2_FILIAL ,
       E1_FILORIG ,
       (E1_NUM || E1_PARCELA) titulo ,
       (E1_CLIENTE || E1_LOJA) idCliente ,
       A1_NOME ,
       E1_EMISSAO ,
       E1_NUM ,
       E1_SERIE ,
       E1_VENCORI ,
       E1_VENCREA ,
       E1_VALLIQ ,
       E1_STATUS ,
       E1_BAIXA ,
       E1_VEND1 ,
       (E1_FILIAL || E1_FILORIG || E1_NUM || E1_SERIE || E1_PARCELA || E1_CLIENTE || E1_LOJA || E1_EMISSAO) id ,
       B.R_E_C_N_O_ ,
       B.D_E_L_E_T_ DELETADO_SE1 ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_INCLUSAO ,
       TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') AS DATA_ALTERACAO
FROM SE1010 B
INNER JOIN SA1010 X ON X.A1_COD = B.E1_CLIENTE
AND X.A1_LOJA = B.E1_LOJA
AND X.D_E_L_E_T_ <> '*'
INNER JOIN SF2010 I ON I.F2_FILIAL = B.E1_FILORIG
AND I.F2_CLIENTE = B.E1_CLIENTE
AND I.F2_LOJA = B.E1_LOJA
AND I.F2_EMISSAO = B.E1_EMISSAO
AND I.D_E_L_E_T_ <> '*'
WHERE B.E1_USERLGI <> ' '
  AND (TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') >= '20210826'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGI, 16, 1)) - 50)), 'YYYYMMDD') <= '20210827'
       OR TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') >= '20210826'
       AND TO_CHAR(TO_DATE('19960101', 'YYYYMMDD') + ((ASCII(SUBSTR(B.E1_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.E1_USERLGA, 16, 1)) - 50)), 'YYYYMMDD') <= '20210827')