SELECT
	F2_FILIAL,
	A1_FILIAL,
(F2_CLIENTE || F2_LOJA) idCliente,
	A1_CGC,
	F2_DOC,
	F2_SERIE,
	F2_EMISSAO,
	F2_STATUS,
	F2_VALMERC,
	F2_VALBRUT,
(
		F2_FILIAL || F2_DOC || F2_SERIE || F2_CLIENTE || F2_LOJA || F2_EMISSAO || F2_CHVNFE || F2_IDNF
	) id,
	B.R_E_C_N_O_,
	B.D_E_L_E_T_ DELETADO_SF2,
	TO_CHAR(
		TO_DATE('19960101', 'YYYYMMDD') + (
			(ASCII(SUBSTR(B.F2_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.F2_USERLGI, 16, 1)) - 50)
		),
		'YYYYMMDD'
	) AS DATA_INCLUSAO,
	TO_CHAR(
		TO_DATE('19960101', 'YYYYMMDD') + (
			(ASCII(SUBSTR(B.F2_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.F2_USERLGA, 16, 1)) - 50)
		),
		'YYYYMMDD'
	) AS DATA_ALTERACAO
FROM
	SF2010 B
	INNER JOIN SA1010 X ON X.A1_COD = B.F2_CLIENTE
	AND X.A1_LOJA = B.F2_LOJA
	AND X.D_E_L_E_T_ <> '*'
WHERE
	B.F2_USERLGI <> ' '
	AND (
		TO_CHAR(
			TO_DATE('19960101', 'YYYYMMDD') + (
				(ASCII(SUBSTR(B.F2_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.F2_USERLGI, 16, 1)) - 50)
			),
			'YYYYMMDD'
		) >= '20210922'
		AND TO_CHAR(
			TO_DATE('19960101', 'YYYYMMDD') + (
				(ASCII(SUBSTR(B.F2_USERLGI, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.F2_USERLGI, 16, 1)) - 50)
			),
			'YYYYMMDD'
		) <= '20210924'
		OR TO_CHAR(
			TO_DATE('19960101', 'YYYYMMDD') + (
				(ASCII(SUBSTR(B.F2_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.F2_USERLGA, 16, 1)) - 50)
			),
			'YYYYMMDD'
		) >= '20210922'
		AND TO_CHAR(
			TO_DATE('19960101', 'YYYYMMDD') + (
				(ASCII(SUBSTR(B.F2_USERLGA, 12, 1)) - 50) * 100 + (ASCII(SUBSTR(B.F2_USERLGA, 16, 1)) - 50)
			),
			'YYYYMMDD'
		) <= '20210924'
	)